import java.util.*;
import static java.lang.System.out;
public class Board
{
  public void loop()
  {
      for(int c=1;c<=7;c++)
      {
        if (c%2==0)
        {
            out.print(" ");
        }
        for(int h=1;h<=7;h++)
        {
            out.print("# ");
        }
        out.println();
      }
  }  
  public static void main(String[] args)
  {
      Board brd=new Board();
      brd.loop();
  }
}
